OR(HaveFlushDraw AND nsuited>nsuitedcommon AND ishipair AND NOT PairOnBoard)) ? 3 :
   ((NOT TwoPairOnBoard AND HaveFlushDraw AND `((~srankbits&0x7fff)>>srankhiplayer)==0 AND FlushPossible AND NOT PairOnBoard) 
OR (NOT TwoPairOnBoard AND HaveFlushDraw AND `((~srankbits&0x7fff)>>srankhiplayer)==0 AND FlushPossible AND nrankedcommon<3) 
