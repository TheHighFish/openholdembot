(f$wheeldraw && NOT PairOnBoard)) ? 1 :
0

##f$wheeldraw##
// !!! Does not consider all wheels
(rankbits>>9==0b111100)                                 // AKQJ
|| (rankbits & 0b0000000000011110)==0b0000000000011110  // 432A

##f$safeboard##
[ NOT PairOnBoard && nsuitedcommon<=2 && nstraightcommon<=2 ]

##f$actionpreflop_1##
