WHEN f$MiddleStage AND (InSmallBlind OR InBigBlind)
   WHEN user_Stack_FullRing_0_2 RaiseMax FORCE

WHEN f$MiddleStage AND user_Stack_FullRing_12P 
   WHEN BotsLastAction = Raise AND listCat1 RaiseMax FORCE
   WHEN BotsLastAction = Raise AND listCat2 RaiseMax FORCE
   WHEN BotsLastAction = Raise Fold FORCE

   WHEN f$FirstAction AND listCat1 RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat2 RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat3 RaiseBy 3 FORCE

   WHEN f$FirstAction AND listCat4 AND Raises = 0 AND Calls >= 0 RaiseBy 3 FORCE

   WHEN f$FirstAction AND listCat4 AND Raises = 0 AND Calls = 0 AND user_EP RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat5 AND Raises = 0 AND Calls = 0 AND user_EP RaiseBy 3 FORCE

   WHEN f$FirstAction AND listCat4 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat5 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat6 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseBy 3 FORCE

   WHEN f$FirstAction AND listCat4 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat5 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat6 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseBy 3 FORCE
   WHEN f$FirstAction AND listCat7 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseBy 3 FORCE

WHEN f$MiddleStage AND user_Stack_FullRing_5_12
   WHEN listCat1 RaiseMax FORCE
   WHEN listCat2 RaiseMax FORCE
   WHEN listCat3 RaiseMax FORCE
   WHEN listCat4 RaiseMax FORCE
   WHEN listCat5 AND Raises = 0 AND Calls >= 0 RaiseMax FORCE

   WHEN listCat5 AND Raises = 0 AND Calls = 0 AND user_EP RaiseMax FORCE
   WHEN listCat6 AND Raises = 0 AND Calls = 0 AND user_EP RaiseMax FORCE

   WHEN listCat5 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseMax FORCE
   WHEN listCat6 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseMax FORCE
   WHEN listCat7 AND Raises = 0 AND Calls = 0 AND (uInMiddlePosition) RaiseMax FORCE

   WHEN listCat5 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseMax FORCE
   WHEN listCat6 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseMax FORCE
   WHEN listCat7 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseMax FORCE 
   WHEN listCat8 AND Raises = 0 AND Calls = 0 AND (InButton OR InSmallBlind) RaiseMax FORCE

WHEN f$MiddleStage AND user_Stack_FullRing_0_5
   WHEN listCat1 RaiseMax FORCE
   WHEN listCat2 RaiseMax FORCE
   WHEN listCat3 RaiseMax FORCE
   WHEN listCat4 RaiseMax FORCE
   WHEN listCat5 RaiseMax FORCE
   WHEN listCat6 AND Raises = 0 AND Calls >= 0 RaiseMax FORCE

   WHEN listCat7 AND Raises = 0 AND Calls = 0 RaiseMax FORCE

   WHEN listCat8 AND Raises = 0 AND Calls = 0 AND NOT (user_EP) RaiseMax FORCE


//////////////////////////////////////////////////////////////////
// Early Game Play
//////////////////////////////////////////////////////////////////
WHEN f$EarlyStage AND OpponentIsAllin AND f$FirstAction AND listCat1 Call FORCE

WHEN f$EarlyStage AND OpponentIsAllin
 // If we entered the hand with a Raise, Call an all-in of 1 BB OR less
   WHEN BotsLastAction = Raise AND AmountToCall <= 1 Call FORCE
 // Get out of the multi-way all in
   WHEN f$FirstAction AND (CallsSinceLastRaise > 0 OR Raises > 1) Fold FORCE

WHEN f$EarlyStage AND (InSmallBlind OR InBigBlind)
   WHEN user_Stack_FullRing_0_2 RaiseMax FORCE

WHEN f$EarlyStage AND listCat1
   WHEN BotsLastAction = Raise RaiseMax FORCE
   WHEN BotsLastAction = None RaiseBy 3 FORCE

WHEN f$EarlyStage AND listCat2
   WHEN BotsLastAction = Raise RaiseMax FORCE
   WHEN BotsLastAction = None RaiseBy 3 FORCE

WHEN f$EarlyStage AND listCat3
   WHEN BotsLastAction = Raise Fold FORCE
   WHEN BotsLastAction = None AND (InCutOff OR InButton) RaiseBy 3 FORCE

WHEN f$EarlyStage AND listCat4
   WHEN BotsLastAction = Raise Fold FORCE
   WHEN BotsLastAction = None AND (InCutOff OR InButton) RaiseBy 3 FORCE

//////////////////////////////////////////////////////////////////
//  Aggression Factor 2 - Call in Small Blind   WHEN 2 OR more callers
//
//  If you see that you bleed too many chips, tighten up the range
//  starting with the last line, AND working your way up
//
//////////////////////////////////////////////////////////////////
WHEN /*!!! Option_Limp_Any_Two_In_SmallBlind */ AND InSmallBlind AND Raises = 0 AND Calls >= 2 AND StackSize > 12
   WHEN listCat3 Call FORCE
   WHEN listCat4 Call FORCE
   WHEN listCat5 Call FORCE
   WHEN listCat6 Call FORCE
   WHEN listCat7 Call FORCE
   WHEN listCat8 Call FORCE
 // this statement will Call with ATC <Any Two Cards> @Trish change this
   WHEN Others Call FORCE

WHEN Others
   WHEN Others Fold FORCE

