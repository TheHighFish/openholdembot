BEGIN_TESTCASE
    TESTCASE_ID ("0060")
    HEURISTIC_RULE (false)
    REASONING ("Range check for symbol balance ")
    PRECONDITION (true)
    POSTCONDITION (gws("balance") >= 0)
    SYMBOLS_POSSIBLY_AFFECTED ("balance ")
END_TESTCASE


