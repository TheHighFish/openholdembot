(RankBitsCommonWithoutPairs >> ace)   & 1 ? ace   :
(RankBitsCommonWithoutPairs >> king)  & 1 ? king  :
(RankBitsCommonWithoutPairs >> queen) & 1 ? queen :
(RankBitsCommonWithoutPairs >> jack)  & 1 ? jack  :
(RankBitsCommonWithoutPairs >> ten)   & 1 ? ten   :
