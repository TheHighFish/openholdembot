WHEN (((f$PostFlop_Senario = 1) AND log$FLOP_Senario_1))
//Eu sou o primeiro a agir
	WHEN ((HaveStraightFlush OR HaveQuads  ) AND log$R_STR8Flush_QUADS) RAISE FORCE 
	WHEN ((HaveFullHouse AND (1 - vs$80$prlos >= .61)) AND log$Raise_FullHouse) RAISE FORCE
	WHEN ((HaveSet AND (1 - vs$80$prlos >= .65)) AND log$R_Set) RAISE FORCE
	WHEN ((HaveTrips AND (1 - vs$80$prlos >= .69)) AND log$R_Trips) RAISE FORCE
	WHEN ((HaveFlush AND (1 - vs$80$prlos >= .86)) AND log$R_Flush) RAISE FORCE
	WHEN ((HaveStraight AND (1 - vs$80$prlos >= .88)) AND log$R_Str8) RAISE FORCE
	WHEN ((HaveTwoPair AND (1 - vs$80$prlos >= .50)) AND log$R_TwoPair) RAISE FORCE
	WHEN ((HavePair AND NOT HaveTrips AND (1 - vs$80$prlos >= .50)) AND log$R_Pair) RAISE FORCE
	WHEN ((OverCards = 2 AND (1 - vs$80$prlos) >= .45) AND log$R_2_OverCards) RAISE FORCE
	WHEN ((OverCards = 1 AND (1 - vs$80$prlos) >= .48) AND log$R_1_OverCards) RAISE FORCE
	WHEN ((HaveStraightDraw  AND (1 - vs$80$prlos) >= .50) AND log$R_Str8Draw) RAISE FORCE
	WHEN ((HaveInsideStraightDraw AND (1 - vs$80$prlos) >= .50) AND log$R_Str8Draw) RAISE FORCE
	WHEN ((HaveFlushDraw AND (1 - vs$80$prlos) >= .45) AND log$R_FlushDraw) RAISE FORCE
	WHEN (Others) FOLD FORCE
	
	
WHEN (((f$PostFlop_Senario = 2) AND log$FLOP_Senario_2))
// 2. Viloes checaram sou o ultimo a agir
	WHEN ((HaveStraightFlush OR HaveQuads  ) AND log$R_STR8Flush_QUADS) RAISE FORCE
	WHEN ((HaveFullHouse AND (1 - vs$80$prlos >= .61)) AND log$Raise_FullHouse) RAISE FORCE
	WHEN ((HaveSet AND (1 - vs$80$prlos >= .65)) AND log$R_Set) RAISE FORCE
	WHEN ((HaveTrips AND (1 - vs$80$prlos >= .69)) AND log$R_Trips) RAISE FORCE
	WHEN ((HaveFlush AND (1 - vs$80$prlos >= .86)) AND log$R_Flush) RAISE FORCE
	WHEN ((HaveStraight AND (1 - vs$80$prlos >= .88)) AND log$R_Str8) RAISE FORCE
	WHEN ((HaveTwoPair AND (1 - vs$80$prlos >= .50)) AND log$R_TwoPair) RAISE FORCE
	WHEN ((HavePair AND NOT HaveTrips AND (1 - vs$80$prlos >= .50)) AND log$R_Pair) RAISE FORCE	
	WHEN ((OverCards = 2 AND (1 - vs$80$prlos) >= .45) AND log$R_2_OverCards) RAISE FORCE
	WHEN ((OverCards = 1 AND (1 - vs$80$prlos) >= .48) AND log$R_1_OverCards) RAISE FORCE
	WHEN ((HaveStraightDraw  AND (1 - vs$80$prlos) >= .50) AND log$R_Str8Draw) RAISE FORCE
	WHEN ((HaveInsideStraightDraw AND (1 - vs$80$prlos) >= .50) AND log$R_InStr8Draw) RAISE FORCE
	WHEN ((HaveFlushDraw AND (1 - vs$80$prlos) >= .45) AND log$R_FlushDraw) RAISE FORCE
	WHEN (Others) FOLD FORCE
	
	
WHEN (((f$PostFlop_Senario = 3) AND log$FLOP_Senario_3))
// 3. Viloes checaram nao sou o ultimo a agir
	WHEN ((HaveStraightFlush OR HaveQuads  ) AND log$R_STR8Flush_QUADS) RAISE FORCE
	WHEN ((HaveFullHouse AND (1 - vs$80$prlos >= .61)) AND log$Raise_FullHouse) RAISE FORCE
	WHEN ((HaveSet AND (1 - vs$80$prlos >= .65)) AND log$R_Set) RAISE FORCE
	WHEN ((HaveTrips AND (1 - vs$80$prlos >= .69)) AND log$R_Trips) RAISE FORCE
	WHEN ((HaveFlush AND (1 - vs$80$prlos >= .86)) AND log$R_Flush) RAISE FORCE
	WHEN ((HaveStraight AND (1 - vs$80$prlos >= .88)) AND log$R_Str8) RAISE FORCE
	WHEN ((HaveTwoPair AND (1 - vs$80$prlos >= .50)) AND log$R_TwoPair) RAISE FORCE
	WHEN ((HavePair AND NOT HaveTrips AND (1 - vs$80$prlos >= .50)) AND log$R_Pair) RAISE FORCE	
	WHEN ((OverCards = 2 AND (1 - vs$80$prlos) >= .45) AND log$R_2_OverCards) RAISE FORCE
	WHEN ((OverCards = 1 AND (1 - vs$80$prlos) >= .48) AND log$R_1_OverCards) RAISE FORCE
	WHEN ((HaveStraightDraw  AND (1 - vs$80$prlos) >= .50) AND log$R_Str8Draw) RAISE FORCE
	WHEN ((HaveInsideStraightDraw AND (1 - vs$80$prlos) >= .50) AND log$R_InStr8Draw) RAISE FORCE
	WHEN ((HaveFlushDraw AND (1 - vs$80$prlos) >= .45) AND log$R_FlushDraw) RAISE FORCE	
	WHEN (Others) FOLD FORCE

WHEN (((f$PostFlop_Senario = 4) AND log$FLOP_Senario_4))
// 4. Vilao fez Raise
	WHEN ((HaveStraightFlush OR HaveQuads  ) AND log$R_STR8Flush_QUADS) RAISE FORCE
	WHEN ((HaveFullHouse AND (1 - vs$80$prlos >= .61)) AND log$Raise_FullHouse) RAISE FORCE
	WHEN ((HaveSet AND (1 - vs$80$prlos >= .65)) AND log$R_Set) RAISE FORCE
	WHEN ((HaveTrips AND (1 - vs$80$prlos >= .69)) AND log$R_Trips) RAISE FORCE
	WHEN ((HaveFlush AND (1 - vs$80$prlos >= .86)) AND log$R_Flush) RAISE FORCE
