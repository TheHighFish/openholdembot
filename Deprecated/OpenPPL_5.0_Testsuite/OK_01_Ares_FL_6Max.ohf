	WHEN ((HaveFlushDraw AND (1 - vs$80$prlos) >= .45) AND log$C_FlushDraw) CALL FORCE
	WHEN (Others) FOLD FORCE

WHEN (((f$PostFlop_Senario = 5) AND log$FLOP_Senario_5))
// 5. Eu fiz raise e recebi ReRaise
	WHEN ((HaveStraightFlush OR HaveQuads  ) AND log$R_STR8Flush_QUADS) RAISE FORCE
	WHEN ((HaveFullHouse AND (1 - vs$80$prlos >= .61)) AND log$CALL_FullHouse) CALL FORCE // preciso melhorar aqui de 61 ate quanto e call
	WHEN ((HaveSet AND (1 - vs$80$prlos >= .851)) AND log$R_Set) RAISE FORCE
	WHEN ((HaveSet AND ((1 - vs$80$prlos >= .65) AND (1 - vs$80$prlos <= .85))) AND log$C_Set) CALL FORCE
