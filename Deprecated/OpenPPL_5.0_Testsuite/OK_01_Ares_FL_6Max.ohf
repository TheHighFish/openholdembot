##f$preflop##

//=========================================================================================================
//=========================================================================================================
//=========================================================================================================

WHEN ((islist01) AND log$OpenR_list01) RAISE FORCE
WHEN (islist01) CALL FORCE
WHEN ((didrais AND ncallbets <= 3) AND log$Call_RR) CALL FORCE// Eu fiz raise e recebi reraise
WHEN ((Raises = 2 AND didrais AND ncallbets <= 4 AND BotsActionsOnThisRoundIncludingChecks = 1) AND log$Call_CAP) CALL FORCE // Call 4bet
WHEN ((didcall AND ncallbets <= 2) AND log$I_Call_C_Raise) CALL FORCE

// Chart Open Raise
WHEN ((( Raises = 0 AND Calls = 0 AND ncallbets <= 1)))
	WHEN ((InSmallBlind AND islist14 ) AND log$OpenR_SB_list14)		 RAISE FORCE
	WHEN ((InButton AND islist13) AND log$OpenR_BTN_list13)			 RAISE FORCE
	WHEN ((InCutOff AND islist12) AND log$OpenR_CO_list12)			 RAISE FORCE
	WHEN ((InMiddlePosition AND islist11) AND log$OpenR_MP_list11)	 RAISE FORCE
	WHEN ((InUTG AND islist10) AND log$OpenR_UTG_list10) 			 RAISE FORCE
	WHEN (f1 AND islist100)  RAISE FORCE

// Raise 1 Limp	
WHEN ((( Raises = 0 AND Calls = 1)))
	WHEN ((islist20) AND log$Raise_1_LIMP) RAISE FORCE 

// Raise/Call 2 Limp	
WHEN ((( Raises = 0 AND Calls = 2)))
	WHEN ((islist21) AND log$Raise_2_LIMP) RAISE FORCE
	WHEN ((islist23) AND log$Call_2_LIMP) CALL FORCE

// Raise/Call 3 Limp	
WHEN ((( Raises = 0 AND Calls = 3)))
	WHEN ((islist22) AND log$Raise_3_LIMP) RAISE FORCE 
	WHEN ((islist24) AND log$CALL_3_LIMP) CALL FORCE

WHEN ((( Raises = 1 AND Calls <= 1)))
	WHEN ((FirstRaiserPosition = 5 AND islist30) AND log$Raise_1_R_UTG) RAISE FORCE 
	WHEN ((FirstRaiserPosition = 4 AND islist32) AND log$Raise_1_R_MP ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 3 AND islist34) AND log$Raise_1_R_CO ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 2 AND islist36) AND log$Raise_1_R_BTN) RAISE FORCE
	WHEN (f0 AND islist100)  RAISE FORCE

//3bet	
WHEN ((( Raises = 1 AND Calls <= 1 AND InSmallBlind)))
	WHEN ((FirstRaiserPosition = 5 AND islist31) AND log$Raise_1_R_UTG_SB) RAISE FORCE 
	WHEN ((FirstRaiserPosition = 4 AND islist33) AND log$Raise_1_R_MP_SB ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 3 AND islist35) AND log$Raise_1_R_CO_SB ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 2 AND islist37) AND log$Raise_1_R_BTN_SB) RAISE FORCE

// Actions against 1 Raise and at least 1 Call
WHEN ((( Raises = 1 AND Calls <= 1)))
	WHEN ((FirstRaiserPosition >= 4 AND (islist40 OR islist41)) AND log$CALL_1_R_UTGMP) CALL FORCE 
	WHEN ((FirstRaiserPosition  = 3 AND (islist40 OR islist42)) AND log$CALL_1_R_CO) CALL FORCE 
	WHEN ((FirstRaiserPosition  = 2 AND islist40) AND log$CALL_1_R_BTN) CALL FORCE 
	
WHEN ((( Raises = 1 AND Calls = 0 AND InBigBlind)))
	WHEN ((FirstRaiserPosition = 5 AND islist55) AND log$RAISE_1_R_UTG_BB) RAISE FORCE 
	WHEN ((FirstRaiserPosition = 4 AND islist56) AND log$RAISE_1_R_MP_BB ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 3 AND islist57) AND log$RAISE_1_R_CO_BB ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 2 AND islist58) AND log$RAISE_1_R_BTN_BB) RAISE FORCE
	WHEN ((FirstRaiserPosition = 1 AND islist59) AND log$RAISE_1_R_SB_BB ) RAISE FORCE
	WHEN ((FirstRaiserPosition = 1 AND islist54) AND log$CALL_1_R_SB_BB ) CALL FORCE 
	WHEN ((FirstRaiserPosition = 2 AND islist53) AND log$CALL_1_R_BTN_BB) CALL FORCE 
	WHEN ((FirstRaiserPosition = 3 AND islist52) AND log$CALL_1_R_CO_BB ) CALL FORCE 
	WHEN ((FirstRaiserPosition = 4 AND islist51) AND log$CALL_1_R_MP_BB ) CALL FORCE 
	WHEN ((FirstRaiserPosition = 5 AND islist50) AND log$CALL_1_R_UTG_BB) CALL FORCE 


