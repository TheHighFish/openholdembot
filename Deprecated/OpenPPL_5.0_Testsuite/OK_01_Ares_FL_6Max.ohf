	WHEN ((HaveTwoPair AND (1 - vs$80$prlos >= .751)) AND log$R_TwoPair) RAISE FORCE
	WHEN ((HavePair AND NOT HaveTrips AND (1 - vs$80$prlos >= .50)) AND log$C_Pair) CALL FORCE
